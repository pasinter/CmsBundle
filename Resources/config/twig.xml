<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="pasinter_cms.collection_extension.class">Pasinter\Bundle\CmsBundle\Twig\CollectionExtension</parameter>
        <parameter key="pasinter_cms.block_extension.class">Pasinter\Bundle\CmsBundle\Twig\BlockExtension</parameter>
    </parameters>

    <services>
        <service id="pasinter_cms.collection_extension" class="%pasinter_cms.collection_extension.class%" public="false">
            <tag name="twig.extension" />
            <call method="setContainer"><argument type="service" id ="service_container" /></call>
        </service>
        
        <service id="pasinter_cms.block_extension" class="%pasinter_cms.block_extension.class%" public="false">
            <tag name="twig.extension" />
            <call method="setContainer"><argument type="service" id ="service_container" /></call>
        </service>

    </services>

</container>
